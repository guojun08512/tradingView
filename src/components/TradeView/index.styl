.el-header
  height 50px
  background-color #fff
  -webkit-box-sizing border-box
  box-sizing border-box
  padding 0 20px
  line-height 50px
  position absolute
  left 0
  top 0
  width 100%
  min-width 1180px
  z-index 2
  -webkit-box-shadow 0 1px 3px #e3e5e6
  box-shadow 0 1px 3px #e3e5e6
  white-space nowrap
  font-size 16px
.el-footer
  border-top 1px solid #e3e5e6
  p
    text-align center
    color #666
    font-size 15px
    font-family Microsoft YaHei
    line-height 60px